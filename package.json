{
  "name": "debridge-data-pipeline",
  "version": "1.0.0",
  "description": "Blockchain data collection and analysis pipeline for USDC transfers",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "start:worker": "node dist/temporal/worker.js",
    "dev": "ts-node src/index.ts",
    "dev:worker": "ts-node src/temporal/worker.ts",
    "collect": "ts-node src/commands/collect.ts",
    "analyze": "ts-node src/commands/analyze.ts",
    "export": "ts-node src/commands/export.ts",
    "pipeline": "ts-node src/commands/pipeline.ts",
    "validate": "ts-node src/commands/validate.ts",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit"
  },
  "keywords": ["blockchain", "ethereum", "usdc", "data-pipeline", "temporal"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@clickhouse/client": "^1.0.1",
    "@temporalio/activity": "^1.10.1",
    "@temporalio/client": "^1.10.1",
    "@temporalio/worker": "^1.10.1",
    "@temporalio/workflow": "^1.10.1",
    "dotenv": "^16.4.5",
    "pino": "^9.0.0",
    "pino-pretty": "^11.0.0",
    "viem": "^2.17.0"
  },
  "devDependencies": {
    "@types/node": "^20.14.0",
    "typescript": "^5.4.5",
    "ts-node": "^10.9.2",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.12",
    "ts-jest": "^29.1.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
